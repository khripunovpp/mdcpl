<!doctype html>
html(lang="ru")
    head

        base(href='../')
        include ../_meta
        block scripts
            include ../_head-scripts

    body
        section.overflow
            include ../_header
            section.tail

                section.lead
                    .container
                        .lead__inner
                            .lead__picture
                                img.lazy(data-src='../img/team-lead-pic.jpg', alt='Наши услуги')
                            .lead__content
                                header.lead__header
                                    button.lead__backBtn(type='button', aria-label='Назад')
                                        i.icon.paste-sprite-arrow-left-icon
                                        span Назад
                                    h1.lead__title Специалисты <br>семейной <br>медицины
            include ../_footer
        include ../_dialogs
        .sprite(aria-hidden="true", style="display: none;")
            include ../../img/sprite.svg


        include ../_footer-scripts.html
        script(defer,src='./js/main.js')
        script(defer,src='./js/libs.min.js',onload="libLoaded()")
